<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:background="@color/colorBackgroundList">

    <LinearLayout
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textViewYearListItem"
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_primary_width"
            android:gravity="center"
            android:text="" />

        <View
            android:id="@+id/dividerListItem1"
            style="@style/DividerBigStyleList" />

        <TextView
            android:id="@+id/textViewStartCapitalListItem"
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_primary_width"
            android:gravity="center"
            android:text="" />

        <View
            android:id="@+id/dividerListItem2"
            style="@style/DividerBigStyleList" />

        <TextView
            android:id="@+id/textViewSalaryListItem"
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_primary_width"
            android:gravity="center"
            android:text="" />

        <TextView
            android:id="@+id/textViewCostsListItem"
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_primary_width"
            android:gravity="center"
            android:text="" />

        <TextView
            android:id="@+id/textViewSavingsListItem"
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_primary_width"
            android:gravity="center"
            android:text="" />

        <View
            android:id="@+id/dividerListItem3"
            style="@style/DividerBigStyleList" />

        <TextView
            android:id="@+id/textViewProfitCapitalListItem"
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_primary_width"
            android:gravity="center"
            android:text="" />

        <TextView
            android:id="@+id/textViewProfitSavingsListItem"
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_primary_width"
            android:gravity="center"
            android:text="" />

        <TextView
            android:id="@+id/textViewPassiveIncomeListItem"
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_primary_width"
            android:gravity="center"
            android:text="" />

        <View
            android:id="@+id/dividerListItem4"
            style="@style/DividerBigStyleList" />

        <TextView
            android:id="@+id/textViewFinalCapitalListItem"
            android:layout_width="match_parent"
            android:layout_height="@dimen/row_primary_width"
            android:gravity="center"
            android:text="" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>