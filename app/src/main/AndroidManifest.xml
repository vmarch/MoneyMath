<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="devtolife.moneymath">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.PrivacyActivity"
            android:label="@string/title_privacy_activity">
        </activity>

        <activity android:name=".view.SettingsActivity"
            android:label="@string/title_settings_activity">
        </activity>

        <activity android:name=".view.AboutAppActivity"
            android:label="@string/title_about_app_activity">
        </activity>
    </application>

</manifest>